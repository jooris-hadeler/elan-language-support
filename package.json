{
  "name": "myriad-language-support",
  "displayName": "Myriad Language Support",
  "version": "1.0.0",
  "description": "Language Support for the Myriad language.",
  "publisher": "jooris-hadeler",
  "repository": {
    "type": "github",
    "url": "https://github.com/jooris-hadeler/myriad-language-support"
  },
  "categories": [
    "Programming Languages"
  ],
  "author": "Jooris Hadeler",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "myriad",
        "aliases": [
          "Myriad",
          "myriad"
        ],
        "extensions": [
          ".myriad",
          ".my"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "myriad",
        "scopeName": "source.myriad",
        "path": "./syntaxes/myriad.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x"
  }
}